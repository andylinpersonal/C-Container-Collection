digraph
{
    rankdir="LR"
    ranksep="1.2"
    nodesep="0.4"
    concentrate="true"
    
    node [ shape="rect" height="0.75!" style="rounded,filled" fillcolor="white" fontname="Inconsolata" fontsize="27" ]

    subgraph cluster_A {  }

    subgraph cluster_B { style="rounded, filled" label="Create" fontname="CMU Serif" fontsize="36" fillcolor="grey95"
                         ccxll_pckd
                         ccxll_type
                         ccxll_extd
                         ccxll_struct_pckd
                         ccxll_struct_extd
                         ccxll_struct
                         ccxll }

    subgraph cluster_C { style="rounded, filled" label="Initialize / Destroy" fontname="CMU Serif" fontsize="36" fillcolor="grey95"
                         ccxll_init
                        _ccxll_init
                         ccxll_init_extd
                         ccxll_iter_init
                        _ccxll_init_extd
                        _ccxll_init_core
                        _ccxll_init_info
                        _ccxll_init_seed
                        _ccxll_iter_init
                         ccxll_free }

    subgraph cluster_D { style="rounded, filled" label="Iterators" fontname="CMU Serif" fontsize="36" fillcolor="grey95"
                         ccxll_iter_copy
                         ccxll_iter_head
                         ccxll_iter_tail
                         ccxll_iter_incr
                         ccxll_iter_decr
                         ccxll_iter_distance
                         ccxll_iter_advance
                         ccxll_iter_at_head
                         ccxll_iter_at_tail 
                         ccxll_iter_begin
                         ccxll_iter_end
                         ccxll_iter_at_begin
                         ccxll_iter_at_end }

    subgraph cluster_E { style="rounded, filled" label="Traversors" fontname="CMU Serif" fontsize="36" fillcolor="grey95"
                         CCXLL_INCR
                         CCXLL_INCR_AUTO
                         CCXLL_INCR_EXTD
                         CCXLL_DECR
                         CCXLL_DECR_AUTO
                         CCXLL_DECR_EXTD }

    subgraph cluster_F { style="rounded, filled" label="Capacity" fontname="CMU Serif" fontsize="36" fillcolor="grey95"
                       { rank="same"
                         ccxll_empty
                         ccxll_size } }

    subgraph cluster_G { style="rounded, filled" label="Modifiers" fontname="CMU Serif" fontsize="36" fillcolor="grey95"

                         subgraph cluster_1 { style="rounded, filled" label="Single" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_insert
                                              ccxll_erase }

                         subgraph cluster_2 { style="rounded, filled" label="Multiple" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_resize
                                              ccxll_append }

                         subgraph cluster_3 { style="rounded, filled" label="Clear" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_clear }
                         
                         subgraph cluster_4 { style="rounded, filled" label="Swap" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_swap }

                         subgraph cluster_5 { style="rounded, filled" label="Basic" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_pop_front
                                              ccxll_pop_back
                                             _ccxll_pop
                                              ccxll_push_front
                                              ccxll_push_back
                                             _ccxll_push }
                       }

    subgraph cluster_H { style="rounded, filled" label="Operations" fontname="CMU Serif" fontsize="36" fillcolor="grey95"


                         subgraph cluster_6 { style="rounded, filled" label="Sort" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_sort
                                              ccxll_sort_extd
                                             _ccxll_sort_extd }

                         subgraph cluster_7 { style="rounded, filled" label="Merge" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_merge
                                              ccxll_merge_extd
                                             _ccxll_merge_extd
                                              ccxll_merge_range
                                              ccxll_merge_range_extd 
                                             _ccxll_merge_range_extd }

                         subgraph cluster_8 { style="rounded, filled" label="Move" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_move
                                              ccxll_move_range
                                              ccxll_move_range_extd }

                         subgraph cluster_9 { style="rounded, filled" label="Reverse" fontname="CMU Serif" fontsize="30" fillcolor="lightgrey"
                                              ccxll_reverse_range }
                       }

    subgraph cluster_I { style="rounded, filled" label="Access" fontname="CMU Serif" fontsize="36" fillcolor="grey95" rank="2"
                         ccxll_front
                         ccxll_back
                       }

    subgraph cluster_J { style="rounded, filled" label="Bitwise\nXOR\nOperations" fontname="CMU Serif" fontsize="36" fillcolor="grey95"
                         XOR2
                         XOR3 }

    subgraph cluster_K { style="rounded, filled" label="Comparator" fontname="CMU Serif" fontsize="36" fillcolor="grey95"
                         ccxll_comp_leq }

    { ccxll_pckd -> ccxll_struct -> ccxll_struct_pckd [style="invis"] }
    { ccxll_iter_init -> ccxll_free [style="invis"] }
    { ccxll_reverse_range -> ccxll_move [style="invis"] }
    { _ccxll_init_seed    -> ccxll_swap [style="invis"] }
    { ccxll_struct_extd   -> ccxll_front -> ccxll_comp_leq -> ccxll_iter_at_end -> ccxll_empty -> XOR3 -> ccxll_merge [style="invis"] }

